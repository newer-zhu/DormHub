<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuhodor.server.mapper.CheckItemLogVoMapper">
    <insert id="saveCheckItemLogs" parameterType="list">
        INSERT INTO
        p_checkitem_log(item_id, score, parent_item, check_log)
        VALUES
        <foreach collection="checkItemLogs" item="item" separator=",">
            (
                #{item.itemId},
                #{item.score},
                #{item.parentItem},
                #{item.checkLog}
            )
        </foreach>
    </insert>
</mapper>